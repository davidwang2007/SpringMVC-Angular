<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hh.business.sale.web.entity">
	<class name="User" table="user" >
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="native" />
		</id>
		<property name="username" type="java.lang.String" column="username"/>
		<property name="password" type="java.lang.String" column="password"/>
		<property name="realname" type="java.lang.String" column="realname"/>
		<property name="age" type="java.lang.Short" column="age"/>
		<property name="gender" type="java.lang.String" column="gender"/>
		<property name="phone" type="java.lang.String" column="phone"/>
		<property name="address" type="java.lang.String" column="address"/>
		<property name="email" type="java.lang.String" column="email"/>
		<property name="createDate" type="java.util.Date" column="create_date"/>
		<property name="updateDate" type="java.util.Date" column="update_date"/>
		<property name="description" type="java.lang.String" column="description"/>
	</class>
</hibernate-mapping>